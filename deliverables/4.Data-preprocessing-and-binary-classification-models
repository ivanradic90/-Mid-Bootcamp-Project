    "#define function\n",
    "def log_transform(x):\n",
    "    if x<=0:\n",
    "        return 0\n",
    "    else:\n",
    "        return np.log(x)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 318,
   "id": "dc120414",
   "metadata": {},
   "outputs": [],
   "source": [
    "# square root transformation\n",
    "def sq_rt_transform(x):\n",
    "    if x<0:\n",
    "        return 0\n",
    "    else:\n",
    "        return x**0.5"
   ]
  },
  {
   "cell_type": "code",
